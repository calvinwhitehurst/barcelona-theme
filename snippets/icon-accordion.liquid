{% liquid
	case size
		when 'small'
			assign icon_size = 33
		when 'medium'
			assign icon_size = 66
		when 'large'
			assign icon_size = 88
		else
			assign icon_size = 66
	endcase
%}
{% if custom_icon != blank or custom_icon_dark != blank %}
	{% if custom_icon != blank %}
		{% assign icon_height = icon_size | divided_by: custom_icon.aspect_ratio %}
		{{
			custom_icon
			| image_url: width: 3840
			| image_tag:
				loading: 'lazy',
				width: icon_size,
				height: icon_height,
				sizes: '100vw (min-width: 767px) 50vw',
				widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
				style: 'object-fit: cover',
				class: 'header__heading-logo header__logo-light'
		}}
	{% endif %}
	{% if custom_icon_dark != blank %}
		{% assign icon_height = icon_size | divided_by: custom_icon_dark.aspect_ratio %}
		{{
			custom_icon_dark
			| image_url: width: 3840
			| image_tag:
				loading: 'lazy',
				width: icon_size,
				height: icon_height,
				style: 'object-fit: cover',
				sizes: '100vw (min-width: 767px) 50vw',
