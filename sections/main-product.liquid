															recommendation.featured_media
															| image_url: width: 3840
															| image_tag:
																loading: 'lazy',
																width: recommendation.featured_media.width,
																height: image_height,
																sizes: '(min-width: 270px) 50vw, 100vw',
																widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
