																	{{ 'products.product.quantity.in_stock' | t: quantity: product.selected_or_first_available_variant.inventory_quantity }}
																{% else %}
																{%- render 'icon-stock-default' -%}
																	{{ 'products.product.quantity.out_of_stock' | t }}
